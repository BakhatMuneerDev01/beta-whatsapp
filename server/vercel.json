{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/socket.io/.*",
            "dest": "server.js",
            "methods": ["GET", "POST"]
        },
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ]
}